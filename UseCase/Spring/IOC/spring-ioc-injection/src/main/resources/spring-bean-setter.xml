<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="student" class="com.sapient.ace.injection.setter.Student">
		<property name="age" value="25" />
		<property name="name" value="student 1" />
		<property name="address" ref="address" />
	</bean>
	
	<bean id="address" class="com.sapient.ace.injection.setter.Address">
		<property name="address" value="My Address" />
		<property name="pinCode" value="231211" />
	</bean>

</beans>
